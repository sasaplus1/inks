{
  "devDependencies": {
    "@tsconfig/strictest": "2.0.5",
    "@typescript-eslint/eslint-plugin": "8.33.1",
    "@typescript-eslint/parser": "8.33.1",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-tsdoc": "0.4.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.5.3",
    "turbo": "2.5.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "22",
    "npm": "11"
  },
  "license": "MIT",
  "packageManager": "npm@11.3.0",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "fix": "run-p -l fix:*",
    "fix:eslint": "ESLINT_USE_FLAT_CONFIG=false eslint --fix .",
    "fix:fixpack": "npx fixpack package.json packages/*/package.json",
    "fix:prettier": "prettier --write .",
    "lint": "run-p -l lint:*",
    "lint:eslint": "ESLINT_USE_FLAT_CONFIG=false eslint .",
    "lint:fixpack": "npx fixpack --dryRun package.json packages/*/package.json",
    "lint:prettier": "prettier --check .",
    "start": "turbo run start",
    "watch": "turbo run watch"
  },
  "workspaces": [
    "packages/*"
  ]
}
