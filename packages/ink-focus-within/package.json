{
  "name": "@sasaplus1/ink-focus-within",
  "description": "simulate :focus-within for ink",
  "version": "0.1.0",
  "devDependencies": {
    "@types/node": "24.10.9",
    "@types/react": "19.1.0",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "ink": "6.3.1",
    "ink-testing-library": "4.0.0",
    "react": "19.1.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  },
  "engines": {
    "node": "24"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "./dist/*"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "peerDependencies": {
    "ink": "6",
    "react": "19"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "directory": "packages/ink-focus-within",
    "type": "git",
    "url": "https://github.com/sasaplus1/inks.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix .",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint .",
    "start": "./index.preview.tsx",
    "test": "vitest run",
    "watch": "tsc --watch"
  },
  "sideEffects": false,
  "source": "./index.tsx",
  "type": "module",
  "types": "./dist/index.d.ts"
}
