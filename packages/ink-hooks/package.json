{
  "name": "@sasaplus1/ink-hooks",
  "description": "hooks for ink",
  "version": "0.1.0",
  "devDependencies": {
    "@types/node": "22.18.6",
    "@types/react": "19.1.0",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-tsdoc": "0.4.0",
    "ink": "6.3.0",
    "ink-testing-library": "4.0.0",
    "react": "19.1.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": "22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "./dist/*"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "peerDependencies": {
    "ink": "6",
    "react": "19"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "directory": "packages/ink-hooks",
    "type": "git",
    "url": "https://github.com/sasaplus1/inks.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix .",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint .",
    "start": "./index.preview.tsx",
    "test": "vitest run",
    "watch": "tsc --watch"
  },
  "sideEffects": false,
  "source": "./index.ts",
  "type": "module",
  "types": "./dist/index.d.ts"
}
